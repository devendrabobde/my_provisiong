<div class="container-fluid">
  <%= form_for([:admin, organization], html: { class: 'form-horizontal organization-form', id: id }) do |f| %>
      <legend><h3><%= text %></h3></legend>
      <div class="row-fluid" style="padding:20px 0px 0px 0px;">
        <div class="span10">
          <!--div class="row-fluid">
          <div class="span12 bgcolor">
            <div class="alert alert-error">
              <a href="#" class="close" data-dismiss="alert">Ã—</a>
              Error Messages.
            </div>
          </div>
          </div-->
          <div class="row-fluid">

            <div class="span4 bgcolor">
              <div class="control-group">
                <%= f.label :name, "Organization Name <span class='red'>*</span>".html_safe, class: "control-label" %>
                  <div class="controls">
                    <%= f.text_field :name, :autofocus => true, class: "form-control", required: "required", placeholder: "Organization Name" %>
                    </div>
                  </div>
                </div><!--/span-->

              </div><!--/row-->


              <div class="row-fluid">

                <div class="span4 bgcolor">
                  <div class="control-group">
                    <%= f.label :address1, "Address1 <span class='red'>*</span>".html_safe, class: "control-label" %>
                      <div class="controls">
                        <%= f.text_field :address1, class: "form-control", placeholder: "Address1" %>
                        </div>
                      </div>
                    </div><!--/span-->

                    <div class="span4 bgcolor">
                      <div class="control-group">
                        <%= f.label :address1, 'Address2', class: "control-label" %>
                          <div class="controls">
                            <%= f.text_field :address2, class: "form-control", placeholder: "Address1" %>
                            </div>
                          </div>
                        </div><!--/span-->

                      </div><!--/row-->

                      <div class="row-fluid">

                        <div class="span4 bgcolor">
                          <div class="control-group">
                            <%= f.label :city, "City <span class='red'>*</span>".html_safe, class: "control-label" %>
                              <div class="controls">
                                <%= f.text_field :city, class: "form-control", placeholder: "City" %>
                                </div>
                              </div>
                            </div><!--/span-->

                            <div class="span4 bgcolor">
                              <div class="control-group">
                                <%= f.label :state_code, "State <span class='red'>*</span>".html_safe, class: "control-label" %>
                                  <div class="controls">
                                    <%#= f.text_field :state_code, class: "form-control", placeholder: "State" %>
                                    <%= f.select(:state_code, options_for_select(US_STATES), :prompt => "Select State") %>
                                    </div>
                                  </div>
                                </div><!--/span-->

                                <div class="span4 bgcolor">
                                  <div class="control-group">
                                    <%= f.label :zip_code, "Zip Code <span class='red'>*</span>".html_safe, class: "control-label" %>
                                      <div class="controls">
                                        <%= f.text_field :zip_code, class: "form-control", required: "required", placeholder: "Zip Code" %>
                                        </div>
                                      </div>
                                    </div><!--/span-->

                                  </div><!--/row-->



                                  <div class="row-fluid">

                                    <div class="span4 bgcolor">
                                      <div class="control-group">
                                        <%= f.label :contact_first_name, "First Name <span class='red'>*</span>".html_safe, class: "control-label" %>
                                          <div class="controls">
                                            <%= f.text_field :contact_first_name, class: "form-control", required: "required", placeholder: "First Name" %>
                                            </div>
                                          </div>
                                        </div>

                                        <div class="span4 bgcolor">
                                          <div class="control-group">
                                            <%= f.label :contact_last_name, "Last Name <span class='red'>*</span>".html_safe, class: "control-label" %>
                                              <div class="controls">
                                                <%= f.text_field :contact_last_name, class: "form-control", required: "required", placeholder: "Last Name" %>
                                                </div>
                                              </div>
                                            </div>



                                          </div><!--row-->

                                          <div class="row-fluid">
                                            <div class="span4 bgcolor">
                                          <div class="control-group">
                                            <%= f.label :contact_email, "Email <span class='red'>*</span>".html_safe, class: "control-label" %>
                                              <div class="controls">
                                                <%= f.text_field :contact_email, class: "form-control", required: "required", placeholder: "Email" %>
                                                </div>
                                              </div>
                                            </div>

                                            <div class="span4 bgcolor">
                                          <div class="control-group">
                                            <%= f.label :contact_phone, "Phone", class: "control-label" %>
                                              <div class="controls">
                                                <%= f.text_field :contact_phone, class: "form-control", placeholder: "Phone" %>
                                                </div>
                                              </div>
                                            </div>

                                            <div class="span4 bgcolor">
                                          <div class="control-group">
                                            <%= f.label :contact_fax, "Fax", class: "control-label" %>
                                              <div class="controls">
                                                <%= f.text_field :contact_fax, class: "form-control", placeholder: "Fax" %>
                                                </div>
                                              </div>
                                            </div>

                                          </div><!--row-->

                                          <div class="row-fluid">

                                            <!--<div class="form-group" style="text-align: right;margin-right:10px">-->

                                            <div class="form-group" style="padding-left:19%;padding-top: 2%">
                                              <div class="col-sm-10">
                                                <%= f.submit :class => "btn btn-primary" %>
                                                <%= link_to "Cancel", admin_organizations_path, class: "btn btn-primary" %>
                                                </div>

                                              </div>


                                            </div><!--/row-->






                                          </div><!--/span-->

                                          <div class="span4">

                                          </div><!--/span-->
                                        </div><!--/row-->
                                      <%- end -%>
                                    </div>
