<table class="table table-striped table-bordered table-condensed" id="table2">
  <thead>
    <tr>
      <th>Last Name</th>
      <th>First Name</th>
      <th>Specialty</th>
      <th> Member Type </th>
      <th> DEA </th>
      <th>NPI</th>
      <th>Status Code</th>
      <th>Status Text </th>
    </tr>
  </thead>
  <tbody>
    <% providers.each do |provider| %>
      <tr>
        <td><%= provider.last_name %></td>
        <td><%= provider.first_name %></td>
        <td><%= provider.specialty %></td>
        <td><%= provider.member_type %></td>
        <td>
          <% if provider.provider_app_detail.provider_dea_numbers.present? %>
            <%= provider.provider_app_detail.provider_dea_numbers.pluck(:provider_dea).join(",") rescue nil %>
          <% end %>
        </td>
       <td><%= provider.npi %></td>
        <td><%= provider.provider_app_detail.status_code %> </td>
        <td><%= provider.provider_app_detail.status_text %></td>
      </tr>
    <% end %>
  </tbody>
</table>