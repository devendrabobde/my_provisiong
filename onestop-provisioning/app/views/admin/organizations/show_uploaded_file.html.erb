<h4> Listing Uploaded Files for <%= @organization.name %></h4>
<p>
  <div>
    <%= select_tag 'application[registered_app_id]', options_from_collection_for_select(@registered_applications, "id", "app_name"), "data-href" => show_uploaded_file_admin_organization_path(@organization.id) %>
  </div>
</p>
<div class="waiting_uploaded_file_image" style="display:none;">
  <%= image_tag("loading.gif") %>
</div>
<div id="uploaded_file">
  <%= render "admin/organizations/uploaded_files", audit_trails: @audit_trails %>
</div>
<br/><br/>
<%= link_to "Back", admin_organizations_path %>
